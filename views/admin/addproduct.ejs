<%- include('../layouts/header.ejs') %>
<div class="container-fluid5">
    <div class="logo-image"><img src="/images (1).jpg" alt=""></div>
<div class="box-addp">
    <h1  class="head1-addp">AddProduct</h1>
    <!-- enctype="multipart/form-data" -->
 
<form action="" class="form1-addp"method="post"  >
    <input type="file" class="form-control2-addp" name="image" required>
    <input type="text" class="form-control1-addp" name="name" placeholder="Enter name of the product" required>
    <br><br>
    <input type="text"  class="form-control1-addp" name="description" placeholder="Enter Description of the product" required>
    <br><br>
    <input type="text"  class="form-control1-addp" name="richDescription" placeholder="Enter RichDescription of the product" required>
    <br><br>
    <input type="text"  class="form-control1-addp" name="brand" placeholder="Enter Brand of the product" required>
    <br><br>
    <input type="number"  class="form-control1-addp" name="price" placeholder="Enter Price of the product" required>
    <br><br>
    <input type="text"  class="form-control1-addp" name="category" placeholder="Enter Category of the product" required>
    <br><br>
  
    
     
    <input type="submit"  class="form-control4-addp" value="Add product">
    
</form>

<a href="/admin/products"  class="form-control4-addp">Go Back</a>
</div>
</div>






